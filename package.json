{
  "name": "vite-plugin-autoedit",
  "version": "0.0.7",
  "description": "A vite plugin to make apps auto-editables",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild"
  },
  "keywords": [
    "vite-plugin"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vite-dev-rpc": "0.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12"
  },
  "peerDependencies": {
    "vite": "^2.9.12"
  }
}
